<div class="container-fluid-full">
  <div class="row-fluid">
    <div class="span9 offset3">
      <div class="row-fluid"></div>
      <%if !blogs.nil?%>
        <%blogs.each do|q| %>
            <%= render :partial => "blogs/blog_each", :locals => { :q => q } %> 
        <%end%>
        <div class="row-fluid">
          <div style="float:right">
            <%= will_paginate blogs %>
        </div>
        </div>
      <%end%>
    </div>
  </div>
</div>