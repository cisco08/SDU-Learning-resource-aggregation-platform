<div class="container-fluid-full">
  <div class="span4"></div>
  <div class="span12">
    <%if !blogs.nil?%>
      <%blogs.each do|q| %>
          <%= render :partial => "blog_each", :locals => { :q => q } %> 
      <%end%>
      <div style="float:right">
          <%= will_paginate blogs %>
      </div>
    <%end%>
  </div>
</div>