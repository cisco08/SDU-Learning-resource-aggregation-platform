<div class="container-fluid">
  <div class="row-fluid">
    <div class="span10 offset2">
      <%if !questions.nil?%>
        <%questions.each do|q| %>
            <%= render :partial => "questions/question_each", :locals => { :q => q } %> 
        <%end%>
        <div style="float:right">
            <%= will_paginate questions %>
        </div>
      <%end%>
    </div>
  </div>
</div>