<div class="container-fluid-full">
  <div class="span4"></div>
  <div class="span12">
    <%if !questions.nil?%>
      <%questions.each do|q| %>
          <%= render :partial => "question_each", :locals => { :q => q } %> 
      <%end%>
      <div style="float:right">
          <%= will_paginate questions %>
      </div>
    <%end%>
  </div>
</div>