<div class="container-fluid-full">
  <div class="span4"></div>
  <div class="span12">
    <%if !resources.nil?%>
      <%resources.each do|q| %>
          <%= render :partial => "resource_each", :locals => { :q => q } %> 
      <%end%>
      <div style="float:right">
          <%= will_paginate resources %>
      </div>
    <%end%>
  </div>
</div>