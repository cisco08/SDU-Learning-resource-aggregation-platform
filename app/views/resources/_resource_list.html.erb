<div class="container-fluid">
  <div class="row-fluid">
    <div class="span10 offset2">
      <%if !resources.nil?%>
        <%resources.each do|q| %>
            <%= render :partial => "resources/resource_each", :locals => { :q => q } %> 
        <%end%>
        <div style="float:right">
            <%= will_paginate resources %>
        </div>
      <%end%>
    </div>
  </div>
</div>